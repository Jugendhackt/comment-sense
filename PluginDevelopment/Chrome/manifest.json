{
  "name": "CommentSense",
  "description": "CommentSense",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "tabs"
    ],
  "icons": { "32": "/assets/icons/icon.png"},
  "browser_action": {
    "default_icon": "/assets/icons/icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "JS/popup.js",
        "JS/backend.js"
      ]
    }
  ]
}
