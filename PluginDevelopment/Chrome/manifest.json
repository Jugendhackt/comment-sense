{
	"name": "CommentSense",
	"description": "CommentSense",
	"version": "0.1",
	"manifest_version": 2,
	"permissions": [
		"activeTab",
		"tabs",
		"storage"
	],
	"options_ui": {
		"page": "HTML/option/options.html",
		"open_in_tab": false
	},
	"icons": { "32": "/assets/icons/icon.png"},
	"browser_action": {
		"default_icon": "/assets/icons/icon.png",
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": [
				"*://*/*"
			],
			"js": [
				"JS/popup.js",
				"JS/backend.js"
			]
		}
	]
}
